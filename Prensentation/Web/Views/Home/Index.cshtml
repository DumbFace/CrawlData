@{
    ViewData["Title"] = "Home Page";
}

@model List<Comic>

<div class="container">

    <table class="table">
        <thead>
            <tr>
                <th>Tên</th>
                <th>Thumb</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td><a href="/@item.Url">@item.Title</a> </td>
                        <td><img src="@item.Thumb" alt="@item.Title"> </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
